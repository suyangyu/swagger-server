{
  "name": "open-api",
  "version": "1.0.0",
  "description": "Simple server to serve openapi files under resources as raw or swagger",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "redoc:static": "npm run build && node dist/redoc-static.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "nodemon": "^3.1.9",
    "redoc-express": "^2.1.0",
    "swagger-ui-express": "^5.0.1",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@redocly/cli": "^1.34.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  },
  "private": true
}
